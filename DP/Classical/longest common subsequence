#include <bits/stdc++.h>

using namespace std;


int main()
{
    string s1, s2; cin>>s1>>s2;
    int sz1=s1.size(); int sz2=s2.size();
    int dp[sz1][sz2];
    memset(dp,0,sizeof(dp));
    if(s1[0]==s2[0]) dp[0][0]=1;
    for(int i=1;i<sz1;i++){
        dp[i][0]=dp[i-1][0];
        if(s1[i]==s2[0]) dp[i][0]++;
    }
    for(int i=1;i<sz2;i++){
        dp[0][i]=dp[0][i-1];
        if(s1[0]==s2[0]) dp[0][i]++;
    }
    for(int i=1;i<sz1;i++){
        for(int j=1;j<sz2;j++){
            if(s1[i]==s2[j]) dp[i][j]=dp[i-1][j-1]+1;
            else dp[i][j]=max(dp[i-1][j], dp[i][j-1]);
        }
    }
    int ans=dp[sz1-1][sz2-1];
    cout<<ans;
    return 0;
}
